{
  "name": "formorama",
  "version": "0.7.4",
  "description": "Advanced form handling for React",
  "author": {
    "name": "Maarten van Ittersum",
    "email": "maarten@v-ittersum.nl"
  },
  "keywords": [
    "react",
    "forms",
    "form",
    "events",
    "dom",
    "html",
    "context",
    "input"
  ],
  "license": "MIT",
  "repository": "dezemand/formorama",
  "main": "dist/formorama.js",
  "module": "dist/formorama.es.js",
  "jsnext:main": "dist/formorama.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "build:js": "rollup -c",
    "build:clean-types": "rimraf dist/types",
    "build": "run-s build:js build:clean-types",
    "start": "rollup -c -w",
    "clean": "rimraf dist",
    "prepare": "run-s clean build",
    "prepublishOnly": "run-s prettier lint test",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "prettier": "prettier --write ./src",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "babel-core": "^6.26.3",
    "babel-jest": "^28.1.3",
    "babel-runtime": "^6.26.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.77.0",
    "rollup-plugin-dts": "^4.2.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "files": [
    "dist"
  ]
}
